{
  "version": "2.0.0",
  "tasks": [
    {
      "taskName": "tsc",
      "command": "tsc",
      "type": "shell",
      "args": ["-p", "jsconfig.json"],
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": true,
        "panel": "shared"
      },
      "problemMatcher": "$tsc",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
